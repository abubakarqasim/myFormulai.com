{
  "name": "myformulai",
  "version": "1.1.0",
  "description": "Enterprise-grade test automation framework built with Playwright and TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:api": "playwright test api/",
    "test:ui-only": "playwright test tests/",
    "test:local": "cross-env ENV=local playwright test",
    "test:dev": "cross-env ENV=dev playwright test",
    "test:staging": "cross-env ENV=staging playwright test",
    "test:prod": "cross-env ENV=prod playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=\"Mobile Chrome\"",
    "test:payments": "playwright test tests/payments/",
    "test:search": "playwright test tests/search/",
    "test:ai": "playwright test tests/ai/",
    "test:performance": "playwright test tests/performance/",
    "test:report": "playwright show-report reports/html-report",
    "test:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "test:allure:generate": "allure generate allure-results --clean -o allure-report",
    "test:allure:open": "allure open allure-report",
    "test:allure:serve": "allure serve allure-results",
    "test:codegen": "playwright codegen",
    "install:browsers": "playwright install",
    "newman:run": "node newman/runner.js",
    "newman:run:report": "node newman/runner.js --report",
    "newman:run:all": "node newman/runner.js --report --json-report --junit-report",
    "newman:list": "node newman/list.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.4.4",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "newman": "^6.2.1",
    "newman-reporter-html": "^1.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
